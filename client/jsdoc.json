{
    "source": {
        "include": ["src"],
        "includePattern": ".(ts|tsx)$",
        "exclude": ["node_modules"]
    },
    "plugins": ["node_modules/jsdoc-babel"],
    "babel": {
        "extensions": ["ts", "tsx"],
        "presets": ["@babel/preset-env", "@babel/preset-typescript"]
    },
    "opts": {
        "destination": "../docs",
        "recurse": true
    }
}